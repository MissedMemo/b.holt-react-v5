{
  "config": {
    "dir": "./4*/src"
  },
  "scripts": {
    "build-dev": "parcel $npm_package_config_dir/index.html",
    "mock-dev": "cross-env PET_MOCK=mock parcel $npm_package_config_dir/index.html",
    "build-prod": "parcel build $npm_package_config_dir/index.html",
    "format": "prettier \"$npm_package_config_dir/**/*.{html,js}\" --write",
    "lint": "eslint \"$npm_package_config_dir/**/*.{js,jsx}\" --quiet"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions",
    "last 2 OperaMobile versions"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@frontendmasters/pet": "^1.0.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
